    "express": "~4.15.2",
    "jade": "~1.11.0",
    "moment": "^2.18.1",
    "morgan": "~1.8.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "rimraf": "^2.6.1"
  }
