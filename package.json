{
  "name": "lunch-crawler",
  "version": "0.0.1",
  "private": true,
  "main": "www.js",
  "scripts": {
    "lint": "standard",
    "clean:coverage": "rimraf coverage/",
    "pretest": "npm run clean:coverage && npm run lint",
    "test": "istanbul cover tape '**/*.spec.*' | tap-spec",
    "serve": "nodemon ./app/bin/www --exec babel-node --inspect",
    "clean:dist": "rimraf dist/",
    "build": "npm run clean:dist && babel ./app --out-dir dist/ --copy-files",
    "start": "node app/bin/www"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cheerio": "^1.0.0-rc.1",
    "co": "^4.6.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "html-entities": "^1.2.1",
    "jade": "~1.11.0",
    "moment": "^2.18.1",
    "morgan": "~1.8.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "serve-favicon": "~2.4.2",
    "striptags": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "istanbul": "^0.4.5",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "standard": "^10.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
